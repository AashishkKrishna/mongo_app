<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
  <appSettings>
    <add key="mongoConnUrl" value="localhost:27017" />
    <add key="MXBusinessDatabaseName" value="MXBusiness" />
    <add key="MXProductCatalogDatabaseName" value="MXProductCatalog" />
    <add key="MXConfigurationDatabaseName" value="MXConfiguration" />
    <add key="rabbitMQConnectionString" value="host=localhost;timeout=60"/>

    <add key="elasticSearchConnectionString" value="http://localhost:9200" />
    <add key="bookIndex" value="mxbook" />
  </appSettings>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  
</configuration>